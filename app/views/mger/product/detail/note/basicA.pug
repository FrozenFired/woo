.row.py-3.border-bottom.rounded
	.col-3
		.title categories
		.desp 产品分类 Array
	.col-9
		each categorie in product.categories
			-a;
			.border.rounded.p-2.mt-2
				each key in Object.keys(categorie)
					- val = categorie[key];
					.row.mt-2
						.col-6 #{key}
						.col-6 #{val}
.row.py-3.border-bottom.rounded
	.col-3
		.title tags
		.desp 产品标签 Array
	.col-9
		each tag in product.tags
			-a;
			.border.rounded.p-2.mt-2
				each key in Object.keys(tag)
					- val = tag[key];
					.row.mt-2
						.col-6 #{key}
						.col-6 #{val}
.row.py-3.border-bottom.rounded
	.col-3
		.title images
		.desp 产品图片 Array
	.col-9
		each image in product.images
			-a;
			.row.border.rounded.p-2.mt-2
				.col-4
					img(src=image.src, width="100%")
				.col-8
					each key in Object.keys(image)
						- val = image[key];
						.row.mt-2
							.col-6 #{key}
							.col-6 #{val}
.row.py-3.border-bottom.rounded
	.col-3
		.title attributes
		.desp 产品属性 Array
	.col-9
		each attribute in product.attributes
			-a;
			.border.rounded.p-2.mt-2
				each key in Object.keys(attribute)
					- val = attribute[key];
					.row.mt-2
						.col-6 #{key}
						.col-6 #{val}
.row.py-3.border-bottom.rounded
	.col-3
		.title.text-danger: span.bg-white default_attributes
		.desp 产品默认属性 Array
	.col-9
		each default_attribute in product.default_attributes
			-a;
			.border.rounded.p-2.mt-2
				each key in Object.keys(default_attribute)
					- val = default_attribute[key];
					.row.mt-2
						.col-6 #{key}
						.col-6 #{val}
.row.py-3.border-bottom.rounded(style="background-color: #F5DEB3")
	.col-3
		.title.text-danger: span.bg-white variations
		.desp 产品变量 Array --- List of variations IDs.
	.col-9
		each variation in product.variations
			-a;
			.border.rounded.p-2.mt-2
				each key in Object.keys(variation)
					- val = variation[key];
					.row.mt-2
